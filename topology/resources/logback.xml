<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>
        %d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n
      </pattern>
    </encoder>
  </appender>
  <logger name="nl.openweb" level="info"
          additivity="false">
    <appender-ref ref="STDOUT"/>
  </logger>
  <root level="warn">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>